<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="mainPane" prefHeight="80.0" prefWidth="150.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1">
    <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <children>
         <GridPane minHeight="30">
           <columnConstraints>
             <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
             <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="30" prefWidth="100.0" />
           </columnConstraints>
           <rowConstraints>
             <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
           </rowConstraints>
            <children>
                  <Label fx:id="lbOpponentNumber" text="Оппонент №N:" textAlignment="CENTER">
                      <font>
                          <Font name="System Bold" size="12.0" />
                      </font>
                  </Label>
               <Button fx:id="btnDelete" maxHeight="Infinity" maxWidth="Infinity" mnemonicParsing="false" text="x" GridPane.columnIndex="1" />
            </children>
         </GridPane>
            <ComboBox fx:id="cbOpponent" maxWidth="Infinity">
            <VBox.margin>
               <Insets top="5.0" />
            </VBox.margin></ComboBox>
            <AnchorPane fx:id="apStatistics" prefHeight="200.0" prefWidth="200.0">
                <VBox.margin>
                    <Insets top="5.0" />
                </VBox.margin>
            <padding>
               <Insets top="5.0" />
            </padding></AnchorPane>
        </children>
        <VBox.margin>
            <Insets top="10.0" />
        </VBox.margin>
    </VBox>
</AnchorPane>
